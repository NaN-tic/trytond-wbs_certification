<form string="WBS Certification">
    <label name="name"/>
    <field name="name"/>
    <label name="date"/>
    <field name="date"/>
    <label name="party"/>
    <field name="party"/>
    <label name="company"/>
    <field name="company"/>
    <label name="wbs"/>
    <field name="wbs"/>
    <button string="Load WBS" name="set_wbs" confirm="Load this WBS?"
        colspan="2"/>
    <newline/>
    <notebook>
        <page name="lines">
            <field name="lines" colspan="4"/>
        </page>
    </notebook>
    <group col="3" colspan="4" id="states">
        <label name="state"/>
        <field name="state"/>
        <group col="4" id="buttons">
            <button name="confirmed" string="Confirm"/>
            <button name="proposal" string="Proposal"/>
            <button string="Reset to Draft" name="draft" icon="tryton-clear"/>
            <button name="cancel" string="Cancel"/>
        </group>
    </group>
</form>

